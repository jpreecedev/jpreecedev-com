{
  "root": true,
  "extends": "next/core-web-vitals",
  "plugins": ["@typescript-eslint", "react", "import", "prettier"],
  "env": {
    "jest": true,
    "es6": true,
    "node": true,
    "browser": true
  },
  "overrides": [
    {
    "files": [
      "*.ts",
      "*.tsx"
    ],
    "rules": {
      "@next/next/no-img-element": 0
    }
    }
  ]
}
